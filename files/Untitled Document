var order: List[Candidate] = Nil
     c.order match {
       case "Numerical" => {
         c.ncandidates match {
           case None => 
           case Some(ncand) => 
            for (i <- (1 to ncand.toInt).reverse) 
              order = new Candidate(i.toString()) :: order
            
         }
       }
       case "ACTGinninderra2004" =>     
